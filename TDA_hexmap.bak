#lang racket


(provide pixbit-d)
(provide get_type)
(provide get_x_hex)
(provide get_y_hex)
(provide get_hex)
(provide get_d_hex)
(provide bitmap?)

; constructor de pixhex-d
; Dominio: pos_x (int) X 

(define (pixhex-d pos_x pos_y hex d)
  (list pos_x pos_y hex d))


; selectores de pixhex

(define (get_type pix)
  (first pix))
(define (get_x_hex pix)
  (second pix))
(define (get_y_hex pix)
  (third pix))
(define (get_hex pix)
  (fourth pix))
(define (get_d_hex pix)
  (fifth pix))
